{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "xo",
    "overrides": [
        {
            "extends": [
                "xo-typescript"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ]
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "camelcase": "off",
      "func-names": "off",
      "new-cap": "off"
    }
}
